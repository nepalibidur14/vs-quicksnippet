{
  "name": "quick-snip-js",
  "displayName": "Quick Snippets for JavaScript, React, TypeScript, and Vue",
  "description": "A powerful JavaScript snippets extension to boost developer productivity. Quickly insert JavaScript, React, TypeScript, and Vue snippets into your code!",
  "version": "0.5.0",
  "publisher": "YAKAZAKI",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nepalibidur14/vs-quicksnippet"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      }
    ]
  },
  "keywords": [
    "javascript",
    "snippets",
    "quick snippets",
    "code snippets",
    "productivity",
    "js",
    "react",
    "typescript",
    "vue",
    "developer tools"
  ]
